<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.hx.ebmapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.hx.ebmapp.MainActivity"
            android:label="Symtompe" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="AdditionalSympActivity"
            android:configChanges="orientation"
            android:label="Zusätzliche Symptome" >
        </activity>
        <activity
            android:name="AdditionalInforsActivity"
            android:configChanges="orientation"
            android:label="Persönliche Daten" >
        </activity>
        <activity
            android:name="DiseasesActivity"
            android:label="Diagnose" >
        </activity>
    </application>

</manifest>